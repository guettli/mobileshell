{
  "threshold": 3,
  "reporters": ["console"],
  "ignore": [
    "**/.git/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/.nix-**/**",
    "**/result/**",
    "**/*_test.go",
    "**/.direnv/**"
